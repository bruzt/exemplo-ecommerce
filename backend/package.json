{
  "name": "ecommerce-backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "start": "npm run prod",
    "dev": "npm-run-all --parallel dev:queue dev:server",
    "dev:server": "cross-env NODE_ENV=development nodemon src/server.js",
    "dev:queue": "cross-env NODE_ENV=development nodemon src/queue.js",
    "test:queue": "cross-env NODE_ENV=test node src/queue.js",
    "preprod": "npm run prod:migrate",
    "prod": "npm-run-all prod:queue prod:server",
    "postprod": "pm2 logs",
    "prod:server": "cross-env NODE_ENV=production pm2 start --name ecommerce-backend -i 2 src/server.js",
    "prod:queue": "cross-env NODE_ENV=production pm2 start --name ecommerce-queue src/queue.js",
    "pretest": "npm run test:migrate",
    "test": "npm-run-all --parallel test:queue test:jest",
    "posttest": "node src/testUtils/deleteDbFile.js",
    "test:jest": "cross-env NODE_ENV=test jest",
    "dev:migrate": "cross-env NODE_ENV=development sequelize db:migrate",
    "prod:migrate": "cross-env NODE_ENV=production sequelize db:migrate",
    "test:migrate": "cross-env NODE_ENV=test sequelize db:migrate",
    "dev:seed": "cross-env NODE_ENV=development sequelize db:seed:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apicache": "^1.6.3",
    "axios": "^0.21.4",
    "bcryptjs": "^2.4.3",
    "bull": "^3.29.2",
    "celebrate": "^12.2.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "frete": "^0.3.2",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.6.5",
    "npm-run-all": "^4.1.5",
    "pagarme": "^4.19.6",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.4",
    "pm2": "^5.1.0",
    "sequelize": "^6.9.0",
    "sequelize-cli": "^6.3.0",
    "socket.io": "^4.2.0",
    "sonic-channel": "^1.2.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/jest": "^25.2.3",
    "eslint": "^8.2.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsdoc": "^37.0.3",
    "factory-girl": "^5.0.4",
    "faker": "^4.1.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.13",
    "socket.io-client": "^4.2.0",
    "sqlite3": "^4.2.0",
    "supertest": "^4.0.2"
  }
}
