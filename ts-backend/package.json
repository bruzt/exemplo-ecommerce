{
  "name": "ts-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:queue dev:server",
    "dev:server": "cross-env NODE_ENV=development ts-node-dev --transpile-only --ignore-watch node_modules --respawn src/server.ts",
    "dev:queue": "cross-env NODE_ENV=development ts-node-dev --transpile-only --ignore-watch node_modules --respawn src/queue.ts",
    "test:queue": "cross-env NODE_ENV=test ts-node-dev --transpile-only --ignore-watch node_modules --respawn src/queue.ts",
    "build": "npm-run-all build:rimraf build:tsc build:copy-views",
    "preprod": "cross-env NODE_ENV=production typeorm migration:run",
    "prod": "npm-run-all prod:queue prod:server",
    "prod:server": "cross-env NODE_ENV=production pm2 start --name ecommerce-backend -i 2 build/src/server.js",
    "prod:queue": "cross-env NODE_ENV=production pm2 start --name ecommerce-queue build/src/queue.js",
    "postprod": "pm2 logs",
    "start": "npm-run-all build prod",
    "dev:migrate": "cross-env NODE_ENV=development ts-node-dev ./node_modules/typeorm/cli.js migration:run",
    "pretest": "cross-env NODE_ENV=test ts-node-dev ./node_modules/typeorm/cli.js migration:run",
    "test": "npm-run-all --parallel test:queue test:jest",
    "test:jest": "cross-env NODE_ENV=test jest",
    "build:rimraf": "rimraf ./build",
    "build:tsc": "tsc",
    "build:copy-views": "copyfiles ./src/views/**/* ./build",
    "dev:seed": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/apicache": "^1.6.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^3.15.4",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/multer": "^1.4.5",
    "@types/node": "^16.9.6",
    "@types/nodemailer": "^6.4.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.28.0",
    "jest": "^27.3.1",
    "rimraf": "^3.0.2",
    "socket.io-client": "^4.2.0",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.7",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "apicache": "^1.6.3",
    "axios": "^0.21.4",
    "bcryptjs": "^2.4.3",
    "bull": "^3.29.2",
    "celebrate": "^13.0.4",
    "concurrently": "^6.2.0",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "frete": "^0.3.2",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.6.5",
    "npm-run-all": "^4.1.5",
    "pagarme": "^4.19.6",
    "pg": "^8.5.1",
    "pm2": "^5.1.0",
    "socket.io": "^4.2.0",
    "sonic-channel": "^1.2.6",
    "typeorm": "^0.2.32",
    "typeorm-seeding": "^1.6.1"
  }
}
